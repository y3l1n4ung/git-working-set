{
  "name": "git-working-set",
  "displayName": "Git Working Set",
  "description": "Shows a focused view of working tree changes in the explorer.",
  "version": "0.0.1",
  "publisher": "Widget-Lab",
  "license": "MIT",
  "engines": {
    "vscode": "^1.74.0"
  },
  "bugs": {
    "url": "https://github.com/y3l1n4ung/git-working-set/issues"
  },
  "homepage": "https://github.com/y3l1n4ung/git-working-set#readme",
  "categories": [
    "SCM Providers",
    "Visualization"
  ],
  "keywords": [
    "git",
    "working set",
    "explorer",
    "changes",
    "scm",
    "vcs",
    "focused view",
    "diff"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/y3l1n4ung/git-working-set"
  },
  "activationEvents": [
    "onView:git-working-set"
  ],
  "extensionDependencies": [
    "vscode.git"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "git-working-set",
          "name": "Working Set",
          "icon": "media/icon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "@types/node": "^22.19.10",
    "@types/vscode": "^1.109.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
